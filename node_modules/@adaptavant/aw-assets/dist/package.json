{
  "name": "@adaptavant/aw-assets",
  "version": "1.3.8",
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack && rimraf css/main.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s lib",
    "clean": "rimraf dist",
    "transpile": "cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,*.test.js,*.stories.jsx,__snapshots__",
    "compile": "npm run clean && npm run transpile && npm-prepare-dist"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adaptavant/aw-assets.git"
  },
  "author": "ragul-r",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Adaptavant/aw-assets/issues"
  },
  "homepage": "https://github.com/Adaptavant/aw-assets#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "module": "./index.js",
  "types": "./index.d.ts"
}